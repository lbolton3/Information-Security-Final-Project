@page
@model SqlInjectionDemo.Pages.LoginModel

@{
    ViewData["Title"] = "Login";
}

<h2>Login</h2>

<form method="post">
    <label>Username:</label>
    <input name="username"/><br/>
    <label>Password:</label>
    <input name="password" type="password"/><br/>
    <button type="submit">Login</button>
</form>

<p>@Model.Message</p>